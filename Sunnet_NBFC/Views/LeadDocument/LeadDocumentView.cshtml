@model IEnumerable<Sunnet_NBFC.Models.clsLeadDocument>
@using Sunnet_NBFC.App_Code
@using Sunnet_NBFC.Models

@*@{
        ViewBag.Title = "Lead Document View";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }*@

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Lead Document View</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    @*<li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Employee View</li>*@

                    @*<h4>@Html.ActionLink("Create New", "StageRole")</h4>*@
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">

                <!-- /.card -->
                <div class="card">
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>
                                        Lead No
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CustomerType)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.CustName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.DocumentName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.IsRequried)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.IsReceived)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Remarks)
                                    </th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.LeadNo)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CustomerType)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CustName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.IsRequried)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.IsReceived)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Remarks)
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                        @*<div class="card-footer">
                                @Html.ActionLink("Export to Excel", "ExportToExcel", new { @class = "btn btn-primary" })
                            </div>*@


                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>


<script src="~/plugins/jquery/jquery.min.js"></script>
<script src="~/plugins/jquery/jquery.validate.min.js"></script>
<script src="~/plugins/jquery/jquery.validate.unobtrusive.min.js"></script>

@if (ViewBag.Error != null && ViewBag.Error != "")
{
    <script type="text/javascript">

            window.onload = function () {
               // alert("@ViewBag.Error");
				swal("TDH", "@ViewBag.Error", "error");

            };
    </script>
}
@if (ViewBag.Success != null && ViewBag.Success != "")
{
    <script type="text/javascript">
            window.onload = function () {
                //alert("@ViewBag.Success");
				swal("TDH", "@ViewBag.Success", "Success");
            };
    </script>
}




