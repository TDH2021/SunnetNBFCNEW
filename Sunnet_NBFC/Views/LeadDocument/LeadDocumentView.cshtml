@model IEnumerable<Sunnet_NBFC.Models.clsLeadDocument>
@using Sunnet_NBFC.App_Code
@using Sunnet_NBFC.Models

@*@{
        ViewBag.Title = "Lead Document View";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }*@

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Lead Document Details</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    @*<li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Employee View</li>*@

                    @*<h4>@Html.ActionLink("Create New", "StageRole")</h4>*@
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">

                <!-- /.card -->
                <div class="card">
                    <!-- /.card-header -->
                    <div class="card-body">
                        <div class="form-group">
                            <table id="example1" class="table table-bordered table-responsive">
                                    <tr>
                                        @*<th>
                                            Lead No
                                        </th>*@
                                        <th>
                                            @Html.DisplayNameFor(model => model.CustomerType)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.CustName)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.DocumentName)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IsRequried)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IsReceived)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Remarks)
                                        </th>

                                    </tr>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            @*<td>
                                                @Html.DisplayFor(modelItem => item.LeadNo)
                                            </td>*@
                                            <td>
                                                @Html.DisplayFor(modelItem => item.CustomerType)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.CustName)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.DocumentName)
                                            </td>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.IsRequried)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IsReceived)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Remarks)
                                            </td>
                                        </tr>
                                    }
                               
                            </table>
                            @*<div class="card-footer">
                                    @Html.ActionLink("Export to Excel", "ExportToExcel", new { @class = "btn btn-primary" })
                                </div>*@
                        </div>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>


